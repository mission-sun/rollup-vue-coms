{
  "presets": [
    [
      "@babel/preset-env", { "modules": false }
    ]
  ],
  "plugins": [
    ["@babel/plugin-syntax-jsx"],
    ["@babel/plugin-proposal-class-properties"],
    ["@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": 2,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ]
}